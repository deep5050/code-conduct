{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": [
        "jsdoc",
        "closure"
      ]
    },
    "source": {
      "include": [
        "index.js"
      ],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "template": "./node_modules/clean-jsdoc-theme",
      "prism-theme": "prism-custom",
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "readme" :"./README.md"
    },
    "plugins": [
      "plugins/markdown",
      "plugins/summarize"
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
    },
    "metadata": {
      "title": "CODE-CONDUCT"
    }
  }